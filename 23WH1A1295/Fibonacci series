//To find Fibonacci series
#include<stdio.h>    
void main()    
{    
 int n1=0,n2=1,n3,i,number;    
 printf("Enter the number of elements:");    
 scanf("%d",&number);  
 printf("Fibonacci series is :") ; 
 printf("\n%d %d",n1,n2);   
 for(i=0;i<number;i++)
 {    
  n3=n1+n2;
  n1=n2;    
  n2=n3;    
  printf(" %d",n3);
 }  
 }
