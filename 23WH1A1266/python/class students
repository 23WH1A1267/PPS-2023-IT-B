class Student:
    def __init__(self, name, age, dob, branch, college, email, phone):
        self.name = name
        self.age = age
        self.dob = dob
        self.branch = branch
        self.college = college
        self.email = email
        self.phone = phone

    def display_bio_data(self):
        print("Name:", self.name)
        print("Age:", self.age)
        print("Date of Birth:", self.dob)
        print("Branch:", self.branch)
        print("College:", self.college)
        print("Email ID:", self.email)
        print("Phone Number:", self.phone)


# Example usage:
if __name__ == "__main__":
    # Creating a Student object
    student1 = Student("John Doe", 20, "2004-05-15", "Computer Science", "ABC College", "john.doe@example.com", "1234567890")

    # Displaying the bio data of the student
    student1.display_bio_data()
