//check whether the given no.is a prime
#include<stdio.h>
int main()
{
  int i,num,count=0;
  scanf("%d",&num);
  printf("\n%d",num);
  i=1;
  while(i<=num)
  {
    if(num%i==0)
    count++;
    i++;
  }
  if(count==2)
  printf("\n Given number is a prime no.");
  else
  printf("\n Given number is not a prime no.");
}
