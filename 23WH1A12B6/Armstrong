include <stdio.h>
int main() 
{
    int num, tem, remainder, result = 0;
    printf("Enter a number ");
    scanf("%d", &num);
    tem = num;
while (tem != 0) 
{
    remainder = tem% 10;
    result += remainder * remainder * remainder
    tem /= 10;
    }
  if (result == num)
        printf("%d is an Armstrong number.", num);
    else
        printf("%d is not an Armstrong number.", num);
}
